<?xml version="1.0"?>
<launch>

	<arg name="fixed_frame" 	value="/map"/>
	<arg name="occupancy_topic" 	value="/move_base/global_costmap/costmap"/>

	<!--param name="/use_sim_time" 	value="true"/-->


	<node name="one_agent_node" pkg="topological_nav" type="one_agent_node" args="-fixed_frame $(arg fixed_frame) -occupancy_topic $(arg occupancy_topic) -rate 10" output="screen"/>

	<!-- rviz launch -->

  	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find topological_nav)/launch/rviz_config/one_agents.rviz"/>

</launch>
